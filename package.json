{
  "name": "calculator",
  "version": "1.0.0",
  "description": "A simple calculator",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "nodemon index.js --exec babel-node --presets es2015,stage-2"
  },
  "keywords": [
    "calculator"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "debug": "^2.6.8",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.6.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
